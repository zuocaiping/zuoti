/**index.wxss**/
page{background: #f6f6f6;padding-bottom:188rpx;}
.m-banner image{width: 100%;height: 320rpx;}
.m-list{background: #fff;margin: 20rpx 20rpx 0; border-radius: 10rpx;padding: 35rpx 28rpx; position: relative;font-size: 40rpx;}
.m-list image{width: 32rpx;height: 32rpx; vertical-align: top;margin: 5rpx 7rpx 0 0;}
.m-progress{ position: relative; width: 88rpx;height: 20rpx;border: 1px solid #b6b6b6;border-radius: 6rpx; display: inline-block;}
.m-progress view{width: 10rpx;height: 20rpx;display: block;background: #b6b6b6;font-size: 20rpx; text-align: center;}
.m-progress text{font-size: 20rpx; text-align: center; position: absolute; width: 100%; line-height: 22rpx; color: #000;}
.m-text{font-size: 30rpx;color: #888888;margin-top: 25rpx;}
.m-text progress{width: 80rpx;height: 20rpx;border-radius: 6rpx; display: inline-block;}
.m-list button{width: 130rpx;font-size: 28rpx;padding: 0rpx 10rpx;position: absolute;right: 25rpx;top:50%;margin-top:-30rpx; line-height: 60rpx;}
.m-tab-title{ height: 168rpx;background: #fff; display: flex; position: fixed;width: 100%;left:0;bottom: 0;z-index: 3;}
.m-tab-title view{flex: 1;text-align: center;border-right:1px solid #e7e7e7;margin-right:-1px;padding-top:12rpx; }
.m-tab-title image{width: 102rpx;height: 102rpx;margin-top: 0rpx; vertical-align: top;}
.m-tab-title text{display: block;font-size: 28rpx; color: #ababab;}
button {
position:relative;
display:block;
margin-left:auto;
margin-right:auto;
padding-left:14px;
padding-right:14px;
box-sizing:border-box;
font-size:18px;
text-align:center;
text-decoration:none;
line-height:2.55555556;
border-radius:5px;
-webkit-tap-highlight-color:none;
overflow:hidden;
color:#000000;
background-color:none;

}
button[type="default"] {
color:#000000;border: none;
background-color:#fff;padding:0;border-radius: 0;height: 80rpx;

}
.button-hover[type="default"] {
color:rgba(0, 0, 0, 0.6);
background-color:#fff;

}


